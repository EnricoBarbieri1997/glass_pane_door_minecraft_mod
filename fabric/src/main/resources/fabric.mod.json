{
	"schemaVersion": 1,
	"id": "glasspanedoormod",
	"version": "${version}",
	"name": "GlassPaneDoorMod",
	"description": "Adds a glass pane door that fits glass panes by sitting in the middle of the block and connecting to them.",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/glasspanedoormod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.enricobarbieri.glasspanedoormod.GlassPaneDoorMod"
		],
		"client": [
			"com.enricobarbieri.glasspanedoormod.GlassPaneDoorModClient"
		]
	},
	"mixins": [
		"glasspanedoormod.mixins.json",
		{
			"config": "glasspanedoormod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}